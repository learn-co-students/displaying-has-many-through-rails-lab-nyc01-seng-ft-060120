<h1><%= @doctor.name %> | <%= @doctor.department %></h1>
<% @doctor.appointments.each do |appoint| %>
    <% @doctor_patient = @doctor.patients.find(appoint.patient_id) %>
    <%= link_to @doctor_patient.name, patient_path(@doctor_patient) %> - <%= link_to appoint.appointment_datetime.strftime("%B %d, %Y at %k:%M"), appointment_path(appoint) %><br>
<% end %>